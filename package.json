{
  "name": "mochawesome-report",
  "version": "0.1.0",
  "description": "A Gorgeous HTML/CSS Reporter for Mocha.js",
  "scripts": {
    "lint": "eslint lib/main.js src",
    "test": "gulp build",
    "test:fn": "node ./test-functional/create-report.js",
    "start": "node ./lib/main.js",
    "build:assets": "rm -rf dist/assets && npm run pack && npm run pack:inline",
    "build:assets:prod": "NODE_ENV=production npm run build:assets",
    "pack": "$(npm bin)/webpack --config ./webpack/external.config.js",
    "pack:inline": "$(npm bin)/webpack --config ./webpack/inline.config.js",
    "dev": "npm run test:fn && open test-functional/reportTest.html && $(npm bin)/webpack-dev-server --config ./webpack/dev.config.js --inline --watch --hot"
  },
  "author": "Adam Gruber",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamgruber/mochawesome-report"
  },
  "keywords": [
    "mocha",
    "reporter",
    "json",
    "react"
  ],
  "engine": "node >= 0.10.0",
  "main": "mochawesome-report.js",
  "dependencies": {
    "chart.js": "^2.3.0",
    "classnames": "^2.2.5",
    "fs-extra": "^0.30.0",
    "highlight.js": "^9.7.0",
    "json-loader": "^0.5.4",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.16.4",
    "material-design-icons": "^3.0.1",
    "mkdirp": "^0.5.1",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "mocha": "*",
    "moment": "^2.15.1",
    "normalize.css": "^5.0.0",
    "ncp": "^2.0.0",
    "postcss": "^5.2.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.8",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mobx-react-devtools": "^4.2.8",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "should": "^11.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.4.2",
    "stylelint-config-standard": "^13.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
